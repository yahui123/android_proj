<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="immersivemode" parent="AppTheme">
        <!--&lt;!&ndash;透明状态栏&ndash;&gt;-->
        <!--<item name="android:windowTranslucentStatus">true</item>-->
        <!--&lt;!&ndash;透明导航栏&ndash;&gt;-->
        <!--<item name="android:windowTranslucentNavigation">true</item>-->
        <!--&lt;!&ndash;使状态栏，导航栏可绘制&ndash;&gt;-->
        <!--<item name="android:windowDrawsSystemBarBackgrounds">true</item>-->
    </style>

    <style name="FullscreenTheme" parent="immersivemode">
        <item name="windowNoTitle">true</item>
        <item name="android:actionBarStyle">@style/FullscreenActionBarStyle</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="metaButtonBarStyle">?android:attr/buttonBarStyle</item>
        <item name="metaButtonBarButtonStyle">?android:attr/buttonBarButtonStyle</item>

    </style>

    <style name="FullscreenActionBarStyle" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>

    <style name="leadMode" parent="AppTheme">
        <item name="android:windowFullscreen">true</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="main" parent="leadMode">
        <item name="android:windowFullscreen">true</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="textview">
        <item name="android:textSize">@dimen/s12</item>
        <item name="android:textColor">#58B3FF</item>
        <item name="android:layout_marginLeft">@dimen/d10</item>
        <item name="android:layout_marginRight">@dimen/d10</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="textview_2">
        <item name="android:textSize">@dimen/s12</item>
        <item name="android:textColor">#58B3FF</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLines">1</item>
    </style>


    <style name="textviewtitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/s12</item>
    </style>

    <style name="textstyle">
        <item name="android:textColor">@color/common_color_blank</item>
    </style>


    <style name="style0">
        <item name="android:textSize">@dimen/s9</item>
    </style>

    <style name="style1">
        <item name="android:textSize">@dimen/s16</item>
    </style>

    <style name="style2">
        <item name="android:textSize">@dimen/sp17</item>
    </style>

    <style name="style4">
        <item name="android:textSize">@dimen/s9</item>
    </style>

    <style name="style3">
        <item name="android:textSize">@dimen/s14</item>
    </style>


    <style name="foodRatingBarBig" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingstars_big</item>
        <item name="android:minHeight">@dimen/d20</item>
        <item name="android:maxHeight">@dimen/d25</item>

    </style>





    <style name="text_title_textsize">
        <item name="android:textColor">@color/common_text_blank</item>
        <item name="android:textSize">@dimen/s16</item>
        <item name="android:paddingTop">@dimen/d20</item>
    </style>

    <style name="my_actionbar_styles">
        <item name="android:layout_height">@dimen/d68</item>
        <item name="android:paddingTop">@dimen/d20</item>
        <item name="android:background">@color/common_white</item>
        <item name="android:orientation">horizontal</item>
    </style>

