    <!--accept-->

    <style name="accept_style_red">
        <item name="android:textColor">@color/accept_text_red</item>
        <item name="android:textSize">@dimen/s13</item>
    </style>


    <style name="text_blue_13sp">
        <item name="android:textColor">@color/rechange_tab_selected</item>
        <item name="android:textSize">@dimen/s13</item>
    </style>

    <style name="text_gray_13">
        <item name="android:textColor">@color/mine_text_gray</item>
        <item name="android:textSize">@dimen/s13</item>
