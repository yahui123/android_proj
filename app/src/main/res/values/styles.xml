    <style name="edittext_style">
        <item name="android:textSize">@dimen/s13</item>
    </style>

    <style name="mine_phone_edit_style">
        <item name="android:textSize">@dimen/s13</item>
        <item name="android:textColor">@color/mine_phone_text_gray</item>
        <item name="android:textColorHint">@color/mine_phone_text_gray_hint</item>
    </style>



    <!--  这个是加入的代码 -->

    <style name="accouptor_popwindow_anim_style">
        <item name="android:windowEnterAnimation">@anim/popup_topenter</item>

        <item name="android:windowExitAnimation">@anim/popup_topexit</item>
    </style>


    <style name="dialog_style" parent="android:style/Theme.Dialog">
        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 没有标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景模糊 -->
    </style>

    <style name="dialog_animatoion_style">
        <item name="android:windowEnterAnimation">@anim/route_in</item>
        <item name="android:windowExitAnimation">@anim/route_out</item>
    </style>


    <!-- 竖屏K线radio -->
    <style name="KLineVRadioButton_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@drawable/radio_btn_line_color_blue_black</item>
        <item name="android:textSize">@dimen/s13</item>
        <item name="android:background">@drawable/radio_btn_line_bg_klin</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
    </style>

    <style name="StyleProgressBarMini" parent="Widget.AppCompat.ProgressBar.Horizontal">

        <item name="android:maxHeight">2dp</item>
        <item name="android:minHeight">2dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">
            @android:drawable/progress_indeterminate_horizontal
        </item>
        <item name="android:progressDrawable">@drawable/horizontal_progress_bar_style</item>

    </style>


    <declare-styleable name="badge_stule">
        <attr name="drawable_badge" format="reference|integer" />
    </declare-styleable>


